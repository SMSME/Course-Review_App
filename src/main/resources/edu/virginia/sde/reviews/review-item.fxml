<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.virginia.sde.reviews.ReviewItemController"
            prefHeight="720.0" prefWidth="1280.0">

    <VBox alignment = "CENTER" spacing = "5.0">
        <HBox alignment="CENTER" spacing = "5.0">
            <Label fx:id="courseMnemonic" text="Course Mnemonic"/>
            <Label fx:id="courseNumber" text = "Course Number"/>
            <Label fx:id="courseRating" text = "Course Rating"/>
        </HBox>
        <Button fx:id="courseReviewButton" mnemonicParsing="false" onAction="#handleClick" text="See Course"/>
    </VBox>


</AnchorPane>
