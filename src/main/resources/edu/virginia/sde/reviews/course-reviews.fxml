<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.virginia.sde.reviews.CourseReviewsController"
            prefHeight="720.0" prefWidth="1280.0">

    <VBox alignment="TOP_LEFT">
        <HBox>
            <Label fx:id="courseLabel" text="Text" style="-fx-font-size: 32px;"/>
            <Label text=" Course Reviews" style="-fx-font-size: 32px;"/>
        </HBox>
        <Button mnemonicParsing="false" onAction="#addReview" text="Add Review"/>
    </VBox>
    <Button mnemonicParsing="false" onAction="#logOut" text="Log Out" AnchorPane.rightAnchor="0"/>
</AnchorPane>

